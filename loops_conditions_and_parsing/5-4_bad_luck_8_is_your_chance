#!/usr/bin/env bash
#sada
a=1
while [ $a -le 10 ]
do
    if [ $a -eq 4 ]; then    # Corrected: space before and after brackets, use $a instead of $i
        echo "bad luck"
    elif [ $a -eq 8 ]; then  # Corrected: space before and after brackets, use $a instead of $i
        echo "good luck"
    else
        echo "Best School"
    fi
    a=$((a + 1))  # Increment the variable a
done
